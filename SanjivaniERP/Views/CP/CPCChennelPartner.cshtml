    function IsMobileNumber() {
        var mob = /^[1-9]{1}[0-9]{9}$/;
        var txtMobile = document.getElementById("mobileNo").value;
        if (txtMobile != '') {
            if (mob.test(txtMobile) == false) {

                $("#mobileNo").val('');
                $("#msg").show();
                document.getElementById("span").innerHTML = "Please enter valid mobile number";
                return false;
            }
            return true;
        }

