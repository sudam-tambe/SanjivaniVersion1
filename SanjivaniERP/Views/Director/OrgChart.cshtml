@model SanjivaniModalView.OrgChart

@{
    ViewBag.Title = "OrgChart";
    Layout = "~/Views/Shared/_LayoutSanjivani.cshtml";
}

<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">Org Chart</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">Org Chart</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<section class="content">
    <div class="container-fluid">
        <!-- Small boxes (Stat box) -->
        <div class="row">

            <div class="col-6 col-sm-6">


                <div class="col-md-12">
                    <!-- USERS LIST -->
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">Channel Partner</h3>

                            <div class="card-tools">
                                
                                <button type="button" class="btn btn-tool" data-card-widget="collapse">
                                    <i class="fas fa-minus"></i>
                                </button>
                                <button type="button" class="btn btn-tool" data-card-widget="remove">
                                    <i class="fas fa-times"></i>
                                </button>
                            </div>
                        </div>
                        <!-- /.card-header -->
                        <div class="card-body p-0" style="display: block;">
                            <ul class="users-list clearfix">
                                @if (ViewBag.CPOrg != null)
                                {
                                    foreach (var item in ViewBag.CPOrg)
                                    {
                                        <li>

                                           
                                            <img src="~/Documents/Logo/@item.ProfilePic" alt="User Image" style="width: 100px;height: 51px;">
                                            <a class="users-list-name" href="#">@item.Name</a>
                                           
                                        </li>
                                    }
                                }
                              

                            </ul>
                            <!-- /.users-list -->
                        </div>
                        <!-- /.card-body -->
                        <div class="card-footer text-center" style="display: block;">
                            <a href="javascript::">View All Users</a>
                        </div>
                        <!-- /.card-footer -->
                    </div>
                    <!--/.card -->
                </div>

            </div>
