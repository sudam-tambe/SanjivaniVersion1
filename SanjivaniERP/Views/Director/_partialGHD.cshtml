        debugger;
        // var id = $(this).attr("id");
        //parts = id.split('.').pop().trim();
        //var currenturl = window.location.href;

        var Url = id
        var url = Url

        debugger;
        $("#pdf").show();
        $("#pdf").attr("src", url);
