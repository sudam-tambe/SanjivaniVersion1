        console.log(response.responseText)
        //$('#messege').text(response.responseText);
       // window.location.href = '@Url.Action("DashBoard", "Dashboard")';
        //$('#messege').text(response.Messege);
    }
    function invTknFailure(response) {
        console.log(response.responseText)
      //  $('#messege').text(response.responseText);
    }

</script>
